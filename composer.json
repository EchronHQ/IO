{
  "name": "echron/io",
  "description": "IO library for PHP",
  "homepage": "https://github.com/echron/io",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Stijn Duynslaeger"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "guzzlehttp/guzzle": "^6.3.3",
    "echron/tools": "^1.1.0",
    "echron/datatypes": "^1.2.0",
    "echron/phpseclib": "dev-keepalive",
    "psr/simple-cache": "^1.0.1",
    "league/flysystem": "^1.0.49"
  },
  "suggest": {
    "kunalvarma05/dropbox-php-sdk": "Use the Dropbox client",
    "google/apiclient": "Use the Google Drive client",
    "aws/aws-sdk-php": "Use the AWS client"
  },
  "autoload": {
    "psr-4": {
      "Echron\\IO\\": "src/Echron/IO"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "cache/array-adapter": "^1.0.1",
    "kunalvarma05/dropbox-php-sdk": "0.2.*",
    "google/apiclient": "^2.0",
    "aws/aws-sdk-php": "3.112.*"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "echron/phpseclib",
        "version": "dev-keepalive",
        "type": "module",
        "source": {
          "url": "https://github.com/Echron/phpseclib.git",
          "type": "git",
          "reference": "keepalive"
        }
      }
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
